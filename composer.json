{
    "name": "PiLep/inpi-rne-php",
    "description": "PHP library to access the INPI RNE API",
    "keywords": ["inpi", "rne", "api", "php"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4" : {
            "InpiRNEClient\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "InpiRNEClient\\Tests\\" : "tests/"
        }
    }
}
