{
    "name": "kanta-inc/inpi-rne-php",
    "description": "PHP library to access the INPI RNE API",
    "keywords": ["inpi", "rne", "api", "php"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.42"
    },
    "autoload": {
        "psr-4" : {
            "RNEClient\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "RNEClient\\Tests\\" : "tests/"
        }
    }
}
